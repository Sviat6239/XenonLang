# Const in Calculation
# Calculates the area of a circle using a constant.

# Declares a constant 'pi' of type float.
const pi: float = 3.14159;
# Declares a mutable variable 'radius' of type float, initialized to 5.
var radius: float = 5;
# Calculates area (pi * radius^2), concatenates with "Circle area: ", and prints.
print("Circle area: " + (pi * radius * radius));
