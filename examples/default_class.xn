# Example of a class without modifiers (defaults to private)
class Student {
    var id: int = 101;  # Private variable
    fun getId(): int {
        return id;
    }
    fun setId(newId: int): void {
        id = newId;
    }
}

# Testing the class
var s: Student = new Student();
print(s.getId());  # Output: 101
s.setId(102);
print(s.getId());  # Output: 102