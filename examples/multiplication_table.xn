# Nested For Loops
#Prints a 3x3 multiplication table.

# Outer loop for rows (1 to 3).
for (var i = 1; i <= 3; i = i + 1) {
    # Inner loop for columns (1 to 3).
    for (var j = 1; j <= 3; j = j + 1) {
        # Prints "i * j = result" for each cell.
        print(i + " * " + j + " = " + (i * j));
    }
}
